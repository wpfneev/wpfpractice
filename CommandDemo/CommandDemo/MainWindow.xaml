<Window x:Class="CommandDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommandDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="500">
    <Window.Resources>
        <local:ViewModel x:Key="vm"/>
    </Window.Resources>
    <Grid>
        <Button x:Name="button" Command="{Binding myProperty, Source={StaticResource vm}}" Content="Button" HorizontalAlignment="Left" Margin="181,151,0,0" 
                VerticalAlignment="Top" Width="164" Height="36" RenderTransformOrigin="-0.065,0.407"/>
    </Grid>
</Window>
