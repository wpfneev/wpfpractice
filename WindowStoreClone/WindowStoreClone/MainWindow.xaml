<Window x:Class="WindowStoreClone.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowStoreClone"
        xmlns:uc="clr-namespace:WindowStoreClone.UserControls"
        mc:Ignorable="d"
        Title="MainWindow" Name="MainWindowMetro" Height="800" Width="1350">
    <Grid>
        <!--<uc:AppViewer></uc:AppViewer>-->
        <!--<uc:TopApp/>-->
        <!--<uc:ProductvityTopApp/>-->
        <Frame x:Name="MainWindowFrame"
               Loaded="MainWindowFrame_Loaded"
               NavigationUIVisibility="Hidden"
               Height="{Binding ElementName=MainWindowMetro, Path=ActualHeight}"
               Width="{Binding ElementName=MainWindowMetro, Path=ActualWidth}"
               ></Frame>
        <!--<uc:AppDetailsTitleAndBackground></uc:AppDetailsTitleAndBackground>-->
    </Grid>
</Window>
